{
    "name": "mobx-fast-form",
    "version": "1.0.2",
    "description": "使用 mobx 实现的轻量级高性能表单组件",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "webpack-dev-server --config config/webpack.config.dev.ts",
        "build": "rm -rf lib && tsc -p config/tsconfig.build.json && cross-env NODE_ENV=production babel lib --out-dir lib",
        "check": "ts-node script/check.ts",
        "release:patch": "npm version patch && npm publish && git push origin master --follow-tags",
        "release:minor": "npm version minor && npm publish && git push origin master --follow-tags",
        "release:major": "npm version major && npm publish && git push origin master --follow-tags"
    },
    "keywords": [
        "mobx",
        "form",
        "mobx-form",
        "mobx-fast-form",
        "mobx-react-form"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/amazecc/mobx-fast-form"
    },
    "author": "Alex Cao",
    "license": "MIT",
